);\n    this.state = {\n      step: +(sessionStorage.getItem(\"step\") || 0),\n    };\n\n    this.continue = this.continue.bind(this);\n  }\n\n  componentDidMount() {\n    const code = new URLSearchParams(this.props.location.search).get(\"step\");\n    this.setState({\n      step: 0,\n    });\n    if(code){\n      this.setState({\n        step: parseInt(code),\n      });\n    }\n    sessionStorage.setItem(\"tutorial\", \"true\");\n  }\n  \n  continue(){\n    const {step} = this.state;\n    sessionStorage.setItem(\"step\", String(step+1))\n    this.redirectTo(step);\n  }\n\n  redirectTo(step: number){\n    if(step === 0){\n      window.location.replace(\"/demo/tutorial?step=1\");\n      this.setState({\n        step: 1,\n      });\n    }\n    if(step === 1){\n      window.location.replace(\"/demo/tutorial?step=2\");\n      this.setState({\n        step: 2,\n      });\n    }\n    if(step === 2){\n      window.location.replace(config.GOVERNMENT_URL);\n    }\n    if(step === 3){\n      window.location.replace(config.UNIVERSITY_URL);\n    }\n    if(step === 4){\n      window.location.replace(\"/demo/tutorial\");\n    }\n  }\n\n  restart(){\n    this.setState({\n      step: 0,\n    });\n    sessionStorage.setItem(\"step\", String(0));\n    window.location.replace(\"/demo/tutorial\");\n  }\nrender() {\n  const {step} = this.state;\n  return (\n    <div className=\"body\">\n      <Header tutorial={true}/>\n      <Grid container \n        direction=\"column\"\n        justify=\"space-between\"\n        alignItems=\"center\"\n        className=\"content\">\n          <Grid item className=\"titleHome\">\n            <Typography variant=\"h4\"><b>{\"Test VIDchain user journey demo by taking the following steps in order\"}</b></Typography>\n          </Grid>\n          <Grid container\n            direction=\"column\"\n            justify=\"space-between\"\n            alignItems=\"center\" \n            className=\"panels\">\n            <Panel \n                title=\"Download VIDwallet\"\n                panelText=\"Find VIDwallet in GooglePlay just clicking on VIDwallet icon. The application is currently available only for Android. An iOS version will be released soon in the App Store.\"\n                stepPanel={0}\n                stepSelected={step}\n                iconOn={downloadWalletIcon}\n                iconOff={downloadWalletIconOff}\n                textButton=\"Next\"\n                functionClickButton={this.continue}/>\n\n            <Panel \n                title=\"Verify your ID\"\n                panelText=\"Once you have innstalled VIDwallet, go to Credentials and create a new credential verifying your ID. By completing this process, you will have verified either your identity card or passport and your liveness. Afterwards, you will receive a Verifiable Credential that you can use to identify yourself later on.\"\n                stepPanel={1}\n                stepSelected={step}\n                iconOn={verifyIcon}\n                iconOff={verifyIDOff}\n                textButton=\"Next\"\n                functionClickButton={this.continue}/> \n\n            <Panel \n                title=\"Government of Freedonia\"\n                panelText=\"Go to Government of Freedonia and sign in using the Verifiable eID that you hold in your wallet. Once you have accesed to the portal you can request the Large Familiy credential that you can use in other entities to apply for discounts.\"\n                stepPanel={2}\n                stepSelected={step}\n                iconOn={governmentIcon}\n                iconOff={governmentIconOff}\n                textButton=\"Go to Freedonia\"\n                functionClickButton={this.continue}/>\n\n            <Panel \n                title=\"ACME University\"\n                panelText=\"Go to ACME University and sign in with VIDchain once more. Here you can request a new credential, namely your Student card. To complete this tutorial, apply for a discount on your student fee presenting the Large Family credential you already hold in your wallet.\"\n                stepPanel={3}\n                stepSelected={step}\n                iconOn={universityIcon}\n                iconOff={universityIconOff}\n                textButton=\"Go to Acme University\"\n                functionClickButton={this.continue}/>\n          </Grid>\n\n          <Grid item className=\"restartButton\">\n              <RestartButton variant=\"contained\" onClick={()=> this.restart()}>\n                  {\"Restart process\"}\n              </RestartButton>\n          </Grid>\n      </Grid>\n\n      \n    </div>\n  );\n  }\n}\n\n\nexport default Tutorial","/Users/alex/Documents/VIDchain-demo-v2/implementations/landing-page/src/components/Header/Header.tsx",["78"],"import React, { Component } from \"react\";\nimport \"./Header.css\";\n\nimport logoVidchain from \"../../assets/images/3531224b-vidchain.svg\";\n\ninterface Props {\n  tutorial: boolean;\n}\n\nfunction Header (props: Props) {\n    const {tutorial} = props;\n    return (\n      <header className=\"headerBackground\">\n        <img\n          className=\"logoHeader\"\n          src={logoVidchain}\n          alt=\"Logo\"\n          onClick={()=> window.location.replace(\"/demo\")}\n        />\n        {tutorial && \n          <p className=\"powered\">Powered by Validated ID</p>\n        }\n        {!tutorial && \n          <a href=\"/demo/tutorial\" className=\"tutorial\">Start tutorial</a>\n        }\n      </header>\n    );\n}\n\nexport default Header;\n","/Users/alex/Documents/VIDchain-demo-v2/implementations/landing-page/src/components/Panel/Panel.tsx",["79"],"/Users/alex/Documents/VIDchain-demo-v2/implementations/landing-page/src/components/NextButton/NextButton.tsx",[],"/Users/alex/Documents/VIDchain-demo-v2/implementations/landing-page/src/components/StartButton/StartButton.tsx",[],"/Users/alex/Documents/VIDchain-demo-v2/implementations/landing-page/src/components/Footer/Footer.tsx",["80","81"],"im